{
  "name": "vetrf-ui",
  "version": "0.1.0",
  "description": "vetrf UI library",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src/",
    "compile": "npm run lint && babel --presets react,env --plugins transform-object-rest-spread  -d lib/ src/components",
    "styleguide-server": "styleguidist server",
    "styleguide-build": "styleguidist build",
    "stlg": "npm run styleguide-build && npm run styleguide-server",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "http://git.vetrf.ru/platform/vetrf-ui.git"
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "library"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-datetime": "^2.12.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.1.1",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.0.0-rc.10",
    "react-tooltip": "^3.4.0",
    "react-scripts": "^1.0.17"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "react-styleguidist": "^6.2.2"
  }
}
