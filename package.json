{
   "name": "vetrf-ui",
   "version": "0.5.0",
   "description": "vetrf UI library",
   "license": "MIT",
   "main": "dist/index.js",
   "module": "dist/index.es.js",
   "jsnext:main": "dist/index.es.js",
   "engines": {
      "node": ">=8",
      "npm": ">=5"
   },
   "typings": "dist/index",
   "scripts": {
      "lint": "eslint --ext .ts,.tsx --fix ./src",
      "test": "npm run lint && jest",
      "build": "rollup -c",
      "start": "rollup -c -w && npm run prettier-watch",
      "prepare": "npm run build",
      "prettier-watch": "onchange 'src/**/*' -- prettier --write {{changed}}",
      "storybook": "start-storybook -p 6006",
      "build-storybook": "build-storybook"
   },
   "dependencies": {
      "moment": "2.24.0",
      "react-datetime": "2.16.3",
      "react-document-title": "2.0.3",
      "react-select": "1.3.0"
   },
   "peerDependencies": {
      "prop-types": "^15.7.2",
      "react": "^16.11.0",
      "react-dom": "^16.11.0",
      "react-router-dom": "^5.1.2",
      "classnames": "^2.2.0"
   },
   "devDependencies": {
      "@babel/core": "^7.7.7",
      "@rollup/plugin-url": "4.0.1",
      "@storybook/addon-actions": "^5.3.0",
      "@storybook/addon-info": "^5.3.0",
      "@storybook/addon-knobs": "^5.3.0",
      "@storybook/addon-links": "^5.3.0",
      "@storybook/addons": "^5.3.0",
      "@storybook/react": "^5.3.0",
      "@types/react-document-title": "^2.0.3",
      "@svgr/rollup": "^5.0.1",
      "@types/classnames": "^2.2.9",
      "@types/enzyme": "^3.10.3",
      "@types/enzyme-adapter-react-16": "^1.0.5",
      "@types/jest": "^24.0.25",
      "@types/jsdom-global": "^3.0.1",
      "@types/lodash": "^4.14.146",
      "@types/node": "^12.11.7",
      "@types/react": "^16.9.17",
      "@types/react-dom": "^16.9.4",
      "@types/react-router-dom": "^4.3.4",
      "@types/react-test-renderer": "^16.9.1",
      "@types/react-tooltip": "^3.11.0",
      "@types/storybook__addon-info": "^5.2.1",
      "@typescript-eslint/eslint-plugin": "^2.15.0",
      "@typescript-eslint/parser": "^2.15.0",
      "awesome-typescript-loader": "5.2.1",
      "babel-eslint": "^10.0.3",
      "babel-loader": "^8.0.6",
      "classnames": "^2.2.6",
      "enzyme": "^3.10.0",
      "enzyme-adapter-react-16": "^1.15.1",
      "eslint": "^6.8.0",
      "eslint-config-prettier": "^6.9.0",
      "eslint-config-react-app": "^5.1.0",
      "eslint-loader": "^3.0.3",
      "eslint-plugin-flowtype": "^4.6.0",
      "eslint-plugin-import": "^2.20.0",
      "eslint-plugin-jsx-a11y": "^6.2.3",
      "eslint-plugin-prettier": "^3.1.2",
      "eslint-plugin-react": "^7.17.0",
      "eslint-plugin-react-hooks": "^2.3.0",
      "husky": "^4.0.6",
      "jest": "^24.9.0",
      "jsdom-global": "3.0.2",
      "lint-staged": "^9.5.0",
      "onchange": "^6.1.0",
      "prettier": "^1.19.1",
      "react": "^16.12.0",
      "react-docgen-typescript": "^1.16.2",
      "react-docgen-typescript-loader": "^3.6.0",
      "react-document-title": "^2.0.3",
      "react-dom": "^16.12.0",
      "react-router-dom": "^5.1.2",
      "react-test-renderer": "^16.12.0",
      "react-tooltip": "^3.11.2",
      "rollup": "^1.29.0",
