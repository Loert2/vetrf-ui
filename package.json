{
   "name": "vetrf-ui",
   "version": "0.1.0",
   "description": "vetrf UI library",
   "main": "lib/index.js",
   "scripts": {
      "lint": "eslint src/ --fix",
      "compile": "npm run lint && babel --presets react,env --plugins transform-object-rest-spread,transform-class-properties  -d lib/ src/components --ignore '**/*.test.js'",
      "styleguide-server": "styleguidist server",
      "styleguide-build": "styleguidist build",
      "stlg": "npm run styleguide-build && npm run styleguide-server",
      "start": "react-scripts start",
      "build": "react-scripts build",
      "test": "react-scripts test --env=jsdom",
      "eject": "react-scripts eject",
      "precommit": "pretty-quick --staged",
      "prettier-watch": "onchange '**/*.js' -- prettier --write {{changed}}"
   },
   "repository": {
      "type": "git",
      "url": "http://git.vetrf.ru/platform/vetrf-ui.git"
   },
   "keywords": [
      "react",
      "ui",
      "components",
      "library"
   ],
   "author": "",
   "license": "MIT",
   "dependencies": {
      "classnames": "^2.2.6",
      "lodash": "^4.17.10",
      "moment": "^2.22.2",
      "prop-types": "^15.6.2",
      "react": "^16.4.1",
      "react-datetime": "^2.15.0",
      "react-document-title": "^2.0.3",
      "react-dom": "^16.4.1",
      "react-router-dom": "^4.3.1",
      "react-scripts": "^1.1.4",
      "react-select": "^1.3.0",
      "react-tooltip": "^3.6.1"
   },
   "devDependencies": {
      "babel-cli": "^6.26.0",
      "eslint-config-prettier": "2.9.0",
      "eslint-plugin-prettier": "2.6.2",
      "husky": "^0.14.3",
      "onchange": "4.1.0",
      "prettier": "1.13.7",
      "pretty-quick": "^1.6.0",
      "react-styleguidist": "^7.1.1"
   }
}
